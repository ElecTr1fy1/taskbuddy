# Supabase (get from supabase.com → Project Settings → API)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Anthropic Claude API (get from console.anthropic.com)
ANTHROPIC_API_KEY=sk-ant-your-key

# Telegram Bot (get from @BotFather on Telegram)
TELEGRAM_BOT_TOKEN=your-bot-token

# Comma-separated Telegram user IDs allowed to use the bot (leave empty for all)
ALLOWED_TELEGRAM_USERS=
